{
  "name": "@trumpsaid/server",
  "version": "1.0.0",
  "license": "CC-BY-NC-4.0",
  "typings": "dist/server",
  "main": "dist/server",
  "repository": {
    "type": "git",
    "url": "https://github.com/trumpsaid-ai/trumpsaid-ai.git"
  },
  "scripts": {
    "build": "tsc -b",
    "build-prod": "yarn build",
    "debug-remote": "node --inspect=0.0.0.0:9229 ./dist/server.js",
    "debug": "nodemon --inspect=9229 ./dist/server.js --ignore ./dist/public",
    "dep-update": "npm-check -u",
    "start": "node ./dist/server.js",
    "lint": "tslint -c ../tslint.json -p tsconfig.json",
    "lint:fix": "tslint -c ../tslint.json -p tsconfig.json --fix",
    "watch": "tsc -w",
    "clean": "rimraf ./dist",
    "serve": "NODE_ENV=production pm2-runtime dist/server.js"
  },
  "dependencies": {
    "@google-cloud/debug-agent": "^3.0.0",
    "@google-cloud/trace-agent": "^3.1.1",
    "@trumpsaid/common": "^1.0.0",
    "@trumpsaid/graphql": "^1.0.0",
    "@trumpsaid/prisma": "^1.0.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "connect-redis": "^3.3.3",
    "csurf": "^1.9.0",
    "dotenv": "^6.0.0",
    "envfile": "^3.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-flash": "0.0.2",
    "express-rate-limit": "^3.1.0",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.0",
    "git-rev-sync": "^1.12.0",
    "lusca": "^1.6.1",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-auth0": "^1.0.0",
    "pug": "^2.0.3",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/csurf": "^1.9.35",
    "@types/express-rate-limit": "^2.9.3",
    "@types/express-session": "^1.15.11"
  }
}
