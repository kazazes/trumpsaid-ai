  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch GraphQL Server",
      "program": "${workspaceFolder}/dist/server.js",
      "restart": true,
      "autoAttachChildProcesses": true,
      "preLaunchTask": "npm: build",
      "sourceMaps": true,
      "smartStep": true,
      "outputCapture": "std"
    }
  ]
}
