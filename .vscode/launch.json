  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch via NPM",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "debug"],
      "port": 9229,
      "sourceMaps": true,
      "outputCapture": "std",
      "protocol": "inspector",
      "restart": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Server",
      "preLaunchTask": "",
      "program": "${workspaceFolder}/dist/server.js",
      "restart": true,
      "sourceMaps": true,
      "outputCapture": "std"
    }
  ]
}
